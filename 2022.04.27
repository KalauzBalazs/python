# Előző órai házi feladat megoldása
from random import randint
def lista_beker():
    lista=[]
    for i in range(5):
        x=randint(0,9)
        lista.append(x)
    return lista

def kiertekel(lista):
    for elem in lista:
        if lista.count(elem) > 1:
            return False
    return True

#Más módon: a set() metódus segítségével!
def set_kiertekel(lista):
    duplum_nelkuli_halmaz=set(lista)
    if len(lista) > len(duplum_nelkuli_halmaz):
        return False
    else:
        return True


for i in range(10):
    lista = lista_beker()
    print(lista)
    if kiertekel(lista):
        print("A lista egy halmaz!")
    else:
        print("A lista NEM halmaz!")
    if set_kiertekel(lista):
        print("A lista egy halmaz!")
    else:
        print("A lista NEM halmaz!")

