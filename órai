x=int(input())
if x<5:
    print('Kisebb mint 5')
elif x>5:
    print('Nagyobb mint 5')
else:
    print('5-tel egyenltő')
    
    
x=int(input())
while x<=5:
    if x<=5:
        print('5 nél nagyobb számot adj meg')
        x=int(input())
else:
    for i in range(1,x+1):
        if i<x:
            print("%4d" % i, end=",")
        else:
            print("%4d" % i, end="")
            
          
x=int(input())
y=int(input())
z=int(input())

if x>y:
    if x>z:
        print('%4d' %x)
    else: print('%4d' %z)
elif y>z:
        print('%4d' %y)
else: print('%4d' %z)


A=int(input("Add meg az első számot: "))
B=int(input("Add meg a második számot: "))


while A<0:
        print('A szám nem lehet negatív!')
        A=int(input("Add meg az első számot: "))
        
while B<=0:
    print('A nullával való osztás nem értelmezhető')
    B=int(input("Add meg a második számot: "))
        
maradek=A

while maradek>=B:
    maradek=maradek-B
    
print("A maradék: %d" % maradek)


a=int(input("Add meg az első számot: "))
b=int(input("Add meg a második számot: "))

while a<=0:
        print('A szám nem lehet negatív!')
        A=int(input("Add meg az első számot: "))
        
if a<b:
    d=a
    a=b
    b=d
    c=a%b
    while c>0:
        a=b
        b=d
        d=a%b
    
lko=b

print("A legnagyobb közös osztó a %d" %lko)


#feldat:
#*****
