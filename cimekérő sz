def legtobb(lista):
    temp=[]
    count=[]
    for i in lista:
        temp.append(i.split(".")[-1])
    print(temp)
    for i in range(len(temp)):
        if (temp.count(temp[i]) > count[0]):
            count[0] = temp.count(temp[i])
            count[1] = i
    print("A legtöbbször előforduló domain végződés: %s" %temp[count[1]])


cimek = []
cim = input("Add meg a címet: ")
while cim != "q":
    cimek.append(cim)
    cim = input("Add meg a címet: ")

legtobb(cimek)
